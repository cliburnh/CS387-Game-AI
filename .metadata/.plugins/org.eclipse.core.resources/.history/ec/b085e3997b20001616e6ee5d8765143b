package behaviorTree;

import java.util.ArrayList;

import ch.idsia.agents.controllers.BehaviorAgent;

public class Tree {
	private ArrayList<Task> children; 
	
	public Tree(Task _first, BehaviorAgent b) {
		children.add(_first);
	}
	
	public void runTree() {
		
	}

	public void addTask(Task _t) {
		children.add(_t);
	}
}
